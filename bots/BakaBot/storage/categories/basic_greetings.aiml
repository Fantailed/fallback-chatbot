<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
  <!-- insert your AIML categories here -->
  
<category>
    <pattern>^ HI ^</pattern>
    <template>
    <think><set name="topic">greetings</set></think>
        <random>
            <li>Hi! Uhh... what's your name?</li>
            <li>Hey :) Umm... who am I talking to?</li>
            <li>Hello :3 Uhh... who are you?</li>
            <li>Hi there :D Uhh... may I know your name?</li>
            <li>Hey ho! Umm... who are you?</li>
            <li>Heyo! How am I supposed to call ya?</li>
            <li>Haha hey ^^ Who is it?</li>
        </random>
    </template>
</category>  

