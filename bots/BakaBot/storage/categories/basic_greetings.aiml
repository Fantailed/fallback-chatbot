<category>
    <pattern>^ MY NAME IS *</pattern>
    <template>
    <think><set name="name"><star index="2"/></set></think>
    <think><set name="topic">*</set></think>
        <random>
            <li>Nice to meet you, <get name="name"/></li>
            <li>Nice to meet ya, <get name="name"/></li>
            <li>Hi, <get name="name"/>, I am <bot name="name"/> :D</li>
            <li><get name="name"/>, that's a nice name :D</li>
            <li>I like your name, <get name="name"/> :3</li>
        </random>
    </template>
</category>

    <topic name="greetings">
    <category>
        <pattern>^ I AM *</pattern>
        <template><srai>MY NAME IS <star index="2"/></srai></template>
    </category>
    <category>
        <pattern>^ IT IS *</pattern>
        <template><srai>MY NAME IS <star index="2"/></srai></template>
    </category>
    <category>
        <pattern>^ CALL ME *</pattern>
        <template><srai>MY NAME IS <star index="2"/></srai></template>
    </category>
    <category>
        <pattern>^ YOU ARE TALKING TO *</pattern>
        <template><srai>MY NAME IS <star index="2"/></srai></template>
    </category>
    <category>
        <pattern>^ NAME IS *</pattern>
        <template><srai>MY NAME IS <star index="2"/></srai></template>
    </category>
    <category>
        <pattern>*</pattern>
        <template><srai>MY NAME IS <star/></srai></template>
    </category>
    </topic>



</aiml>